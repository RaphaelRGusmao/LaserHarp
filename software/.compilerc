{
  "env": {
    "development": {
      "application/javascript": {
        "presets": [
          "@babel/preset-env",
          "@babel/preset-react"
        ],
        "plugins": [
          "@babel/plugin-transform-runtime",
          "@babel/plugin-transform-async-to-generator",
          "@babel/plugin-proposal-class-properties",
          "@babel/plugin-transform-classes",
          "react-hot-loader/babel"
        ],
        "sourceMaps": "inline"
      }
    },
    "production": {
      "application/javascript": {
        "presets": [
          "@babel/preset-env",
          "@babel/preset-react"
        ],
        "plugins": [
          "@babel/plugin-transform-runtime",
          "@babel/plugin-transform-async-to-generator",
          "@babel/plugin-proposal-class-properties",
          "@babel/plugin-transform-classes"
        ],
        "sourceMaps": "none"
      }
    }
  }
}
